{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_LEMONSQUEEZY_STORE_ID": "${REACT_APP_LEMONSQUEEZY_STORE_ID}",
    "REACT_APP_LEMONSQUEEZY_MONTHLY_VARIANT_ID": "${REACT_APP_LEMONSQUEEZY_MONTHLY_VARIANT_ID}",
    "REACT_APP_LEMONSQUEEZY_YEARLY_VARIANT_ID": "${REACT_APP_LEMONSQUEEZY_YEARLY_VARIANT_ID}",
    "REACT_APP_LEMONSQUEEZY_MONTHLY_URL": "${REACT_APP_LEMONSQUEEZY_MONTHLY_URL}",
    "REACT_APP_LEMONSQUEEZY_YEARLY_URL": "${REACT_APP_LEMONSQUEEZY_YEARLY_URL}"
  }
} 